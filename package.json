{
  "name": "planningtool",
  "private": true,
  "devDependencies": {
    "css-slam": "^1.1.0",
    "del": "^2.2.1",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-html-minifier": "^0.1.8",
    "gulp-if": "^2.0.1",
    "gulp-imagemin": "^3.0.1",
    "gulp-style-modules": "^0.1.0",
    "gulp-uglify": "^2.0.0",
    "merge-stream": "^1.0.0",
    "plylog": "^0.4.0",
    "polymer-build": "^0.4.0",
    "polyserve": "git+https://github.com/timvdLippe/polyserve.git#proxy"
  },
  "scripts": {
    "postinstall": "cd ./node_modules/polyserve && npm install && npm run build",
    "build": "node ./node_modules/gulp/bin/gulp.js",
    "serve": "node ./node_modules/polyserve/bin/polyserve -P h2 --proxy-root v1 --proxy-target https://localhost:9000",
    "serve-build": "node ./node_modules/polyserve/bin/polyserve --root build/unbundled/ -P h2 --proxy-root v1 --proxy-target https://localhost:9000"
  },
  "engines": {
    "node": ">=0.10.0"
  }
}
